#include<bits/stdc++.h>
using namespace std;
int main()
{
    long long T;
    cin>>T; 
    string str1 = "codeforces";
    while(T--) {
        string str2;
        cin>>str2;
        int cnt = 0;
        for(int i=0; i<10; i++)
            if(str1[i] != str2[i])
                cnt++;
        cout<<cnt<<endl;
    }
return 0;
}
