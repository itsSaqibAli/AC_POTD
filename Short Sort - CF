#include<bits/stdc++.h>
using namespace std;
int main()
{
    long long T;
    cin>>T; 
    while(T--) {
        string str;
        cin>>str;
        string abc = "abc";
        int cnt = 0;
        for(int i=0; i<3; i++)
            if(abc[i] == str[i])  cnt++;      
        (cnt) ? cout<<"YES\n" : cout<<"NO\n";
    }
    
return 0;
}
