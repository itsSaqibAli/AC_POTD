//User function template for C++
class Solution{
public:	
	// Function returns the second
	// largest elements
	int print2largest(int arr[], int n) {
	    int m=arr[0], s=-1;
	    for(int i=1; i<n; i++)
	    {
	        if(arr[i]>m){
	            s = m;
	            m=arr[i];
	        }
	        else if(arr[i]<m&&arr[i]>s)
	        s=arr[i];
	        
	    }
	    return s;
	}
};
